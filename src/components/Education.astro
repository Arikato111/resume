---
import info from "../data/info.json";
---

<div class="my-2">
  <h3 class="font-bold text-xl">Education</h3>
  <div>
    {
      info.Education.map((edu) => (
        <div>
          <div class="flex justify-between">
            <div class="font-bold">{edu.level},</div>
            <div>{edu.years}</div>
          </div>
          <div class="mx-10"> {edu.school}</div>
        </div>
      ))
    }
  </div>
</div>
